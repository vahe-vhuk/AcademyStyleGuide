\u0561\\u0581\\u0576\\u0565\\u056C \\u0561\\u0575\\u056C\\u0587\\u057D \\u0578\\u0579 \\u0561\\u0576\\u0570\\u0580\\u0561\\u056A\\u0565\\u0577\\u057F #include \\u0570\\u0561\\u0575\\u057F\\u0561\\u0580\\u0561\\u0580\\u0578\\u0582\\u0569\\u0575\\u0578\\u0582\\u0576\\u0576\\u0565\\u0580\\u0568 \\u056B\\u0580\\u0565\\u0576\\u0581 \\u057E\\u0565\\u0580\\u0576\\u0561\\u0563\\u0580\\u0565\\u0580\\u056B\\u0581\\u055D \\u0561\\u057C\\u0561\\u0576\\u0581 client\\u058A\\u0576\\u0565\\u0580\\u056B\\u0576 \\u056F\\u0578\\u057F\\u0580\\u0565\\u056C\\u0578\\u0582: \\u054D\\u0561 \\u057E\\u0565\\u0580\\u0561\\u0562\\u0565\\u0580\\u0578\\u0582\\u0574 \\u0567 \\u0576\\u0561\\u0587 \\u056F\\u0561\\u057A\\u0561\\u056F\\u0581\\u057E\\u0561\\u056E \\u057E\\u0565\\u0580\\u0576\\u0561\\u0563\\u0580\\u0565\\u0580\\u056B\\u0576.\", /*#__PURE__*/_jsxDEV(Bold, {\n          children: \"foo.cc\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 77,\n          columnNumber: 21\n        }, this), \"-\\u0576 \\u057A\\u0565\\u057F\\u0584 \\u0567 \\u0576\\u0565\\u0580\\u0561\\u057C\\u056B \", /*#__PURE__*/_jsxDEV(Bold, {\n          children: \"bar.cc\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 77,\n          columnNumber: 63\n        }, this), \", \\u0565\\u0569\\u0565 \\u0561\\u0575\\u0576 \\u0585\\u0563\\u057F\\u0561\\u0563\\u0578\\u0580\\u056E\\u0578\\u0582\\u0574 \\u0567 \\u0564\\u0580\\u0561\\u0576\\u056B\\u0581 \\u057D\\u056B\\u0574\\u057E\\u0578\\u056C, \\u0576\\u0578\\u0582\\u0575\\u0576\\u056B\\u057D\\u056F \\u0565\\u0569\\u0565 \", /*#__PURE__*/_jsxDEV(Bold, {\n          children: \"foo.cc\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 78,\n          columnNumber: 25\n        }, this), \"-\\u0576 \\u0576\\u0565\\u0580\\u0561\\u057C\\u0578\\u0582\\u0574 \\u0567 bar.h:\"]\n      }, void 0, true, {\n        fileName: _jsxFileName,\n        lineNumber: 70,\n        columnNumber: 17\n      }, this), /*#__PURE__*/_jsxDEV(Subsection, {\n        title: \"Forward Declarations\",\n        children: \"\\u0540\\u0576\\u0561\\u0580\\u0561\\u057E\\u0578\\u0580\\u0578\\u0582\\u0569\\u0575\\u0561\\u0576 \\u0564\\u0565\\u057A\\u0584\\u0578\\u0582\\u0574 \\u056D\\u0578\\u0582\\u057D\\u0561\\u0583\\u0565\\u0584 forward declaration-\\u0576\\u0565\\u0580 (\\u057E\\u0565\\u0580\\u0561\\u0570\\u0561\\u057D\\u0581\\u0565\\u0561\\u0563\\u0580\\u0565\\u0580) \\u0585\\u0563\\u057F\\u0561\\u0563\\u0578\\u0580\\u056E\\u0565\\u056C\\u0578\\u0582\\u0581: \\u0553\\u0578\\u056D\\u0561\\u0580\\u0565\\u0576\\u0568, \\u0576\\u0565\\u0580\\u0561\\u057C\\u0565\\u0584 \\u0571\\u0565\\u0566 \\u0561\\u0576\\u0570\\u0580\\u0561\\u056A\\u0565\\u0577\\u057F \\u057E\\u0565\\u0580\\u0576\\u0561\\u0563\\u0580\\u0565\\u0580\\u0568\"\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 81,\n        columnNumber: 17\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 41,\n      columnNumber: 13\n    }, this)]\n  }, void 0, true, {\n    fileName: _jsxFileName,\n    lineNumber: 19,\n    columnNumber: 9\n  }, this);\n}\n_c = CppContent;\nexport default CppContent;\nvar _c;\n$RefreshReg$(_c, \"CppContent\");","map":{"version":3,"names":["CustomCodeBlock","Section","Subsection","Bold","jsxDEV","_jsxDEV","code1","CppContent","className","children","title","fileName","_jsxFileName","lineNumber","columnNumber","lang","code","_c","$RefreshReg$"],"sources":["/Users/picsartacademy/PycharmProjects/untitled/src/pages/src/cpp.js"],"sourcesContent":["import \"../styles/langpage.css\"\nimport CustomCodeBlock from \"../../components/codeblock/CodeBlock\";\nimport {Section, Subsection} from \"../../components/section/section\";\nimport {Bold} from \"../../components/Bold/Bold\";\n\n\n\nconst code1 = \"#ifndef FOO_BAR_BAZ_H_\\n\" +\n    \"#define FOO_BAR_BAZ_H_\\n\" +\n    \"\\n\" +\n    \"...\\n\" +\n    \"\\n\" +\n    \"#endif  // FOO_BAR_BAZ_H_\"\n\n\n\nfunction CppContent() {\n    return (\n        <div className=\"langpage\">\n            <Section title=\"Ներածություն\">\n                C++-ը զարգացման հիմնական լեզուներից մեկն է, որն օգտագործվում է բաց կոդով (open-source)\n                նախագծերից շատերի կողմից: Ինչպես յուրաքանչյուր C++ ծրագրավորող գիտի, լեզուն ունի բազմաթիվ\n                հզոր առանձնահատկություններ, բայց այդ հզորությունն էլ հանգեցնում է բարդության, ինչն էլ իր\n                հերթին կոդը դարձնում է սխալների հակված` դժվար կարդալու ու պահպանելու տեսանկյունից։<br/><br/>\n\n                Այս ուղեցույցի նպատակն է կառավարել այդ բարդությունը՝ մանրամասն նկարագրելով C++֊ով կոդ գրելու\n                կարելիները և չկարելիները։ Այս կանոնների նպատակն է պահել կոդերի բազան կառավարելի՝ միաժամանակ\n                թույլ տալով ծրագրավորողներին արդյունավետ օգտագործել C++ լեզվի առանձնահատկությունները:<br/><br/>\n\n                Ոճը,այն է, ինչ մենք անվանում ենք մեր C++֊ով կոդը կարգավորող կոնվենցիաներին: Ոճը փոքր ինչ\n                սխալ տերմին է, քանի որ այս կոնվենցիաները ներառում են շատ ավելին, քան պարզապես source ֆայլի\n                ձևաչափումը (formatting):<br/><br/>\n\n                Google-ի կողմից մշակված բաց կոդով (open-source) նախագծերի մեծ մասը համապատասխանում է\n                այս ուղեցույցի պահանջներին:<br/><br/>\n\n                Նկատի ունեցեք, որ այս ուղեցույցը C++֊ի ձեռնարկ չէ. մենք ենթադրում ենք, որ ընթերցողը ծանոթ է լեզվին:<br/><br/>\n\n            </Section>\n\n            <Section title=\"Վերնագրային (Header) ֆայլեր\">\n                Ընդհանուր առմամբ, յուրաքանչյուր .cpp(.cc) ֆայլ պետք է ունենա համապատասխան .h ֆայլ:\n                Կան որոշ ընդհանուր բացառություններ, ինչպիսիք են unit թեստերը և փոքր .cpp(.cc) ֆայլերը,\n                որոնք պարունակում են ընդամենը main() ֆունկցիա:<br/><br/>\n\n                Վերնագրային ֆայլերի ճիշտ օգտագործումը կարող է հսկայական տարբերություն ստեղծել ձեր կոդի\n                ընթեռնելիության, չափի և կատարողականի վրա:<br/><br/>\n\n                Հետևյալ կանոնները ձեզ կառաջնորդեն վերնագրային ֆայլերի օգտագործման տարբեր ծուղակների\n                միջից դուրս գալու համար:\n\n\n                <Subsection title=\"The #define Guard\">\n                    Բոլոր վերնագրային ֆայլերը պետք է ունենան #define guard-ներ, բազմակի ներառումից\n                    խուսափելու համար: Սիմվոլային անվան ձևաչափը պետք է լինի<br/><br/>\n\n                    <Bold>\n                        &lt;PROJECT&gt;_&lt;PATH&gt;_&lt;FILE&gt;_H_\n                    </Bold><br/><br/>\n\n\n\n                    Եզակիությունը երաշխավորելու համար դրանք պետք է հիմնված լինեն նախագծի սկզբնական\n                    ծառի ամբողջական ուղու վրա: Օրինակ, <Bold children=\"foo/src/bar/baz.h\"/> ֆայլը <Bold children=\"project foo\"/>-ում պետք է\n                    ունենա հետևյալ պաշտպանությունը.<br/><br/>\n\n                    <CustomCodeBlock lang=\"cpp\" code={code1}/>\n                </Subsection>\n\n                <Subsection title=\"Include What You Use\">\n                    Եթե որևէ source կամ վերնագրային(header) ֆայլը վերաբերում է այլ վայրում սահմանված սիմվոլին, ֆայլը\n                    պետք է ուղղակիորեն ներառի վերնագրային(header) ֆայլ, որը պատշաճ կերպով նախատեսում է ապահովել այդ սիմվոլի\n                    հայտարարությունը կամ սահմանումը: Այն չպետք է ներառի header ֆայլերը որևէ այլ պատճառով:<br/><br/>\n\n                    Մի՛ ապավինիր անցումային ընդգրկման։ Սա թույլ է տալիս մարդկանց հեռացնել այլևս ոչ անհրաժեշտ #include հայտարարությունները\n                    իրենց վերնագրերից՝ առանց client֊ներին կոտրելու: Սա վերաբերում է նաև կապակցված վերնագրերին.\n                    <Bold children=\"foo.cc\"/>-ն պետք է ներառի <Bold children=\"bar.cc\"/>, եթե այն օգտագործում է դրանից սիմվոլ, նույնիսկ\n                    եթե <Bold children=\"foo.cc\"/>-ն ներառում է bar.h:\n                </Subsection>\n\n                <Subsection title=\"Forward Declarations\">\n                    Հնարավորության դեպքում խուսափեք forward declaration-ներ (վերահասցեագրեր) օգտագործելուց:\n                    Փոխարենը, ներառեք ձեզ անհրաժեշտ վերնագրերը\n                </Subsection>\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n            </Section>\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n        </div>\n    );\n}\n\n\nexport default CppContent;"],"mappings":";AAAA,OAAO,wBAAwB;AAC/B,OAAOA,eAAe,MAAM,sCAAsC;AAClE,SAAQC,OAAO,EAAEC,UAAU,QAAO,kCAAkC;AACpE,SAAQC,IAAI,QAAO,4BAA4B;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAIhD,MAAMC,KAAK,GAAG,0BAA0B,GACpC,0BAA0B,GAC1B,IAAI,GACJ,OAAO,GACP,IAAI,GACJ,2BAA2B;AAI/B,SAASC,UAAUA,CAAA,EAAG;EAClB,oBACIF,OAAA;IAAKG,SAAS,EAAC,UAAU;IAAAC,QAAA,gBACrBJ,OAAA,CAACJ,OAAO;MAACS,KAAK,EAAC,0EAAc;MAAAD,QAAA,GAAC,usDAIwD,eAAAJ,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,05CAIP,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,+6BAIvE,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,0fAGP,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,weAE8D,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC;IAAA;MAAAH,QAAA,EAAAC,YAAA;MAAAC,UAAA;MAAAC,YAAA;IAAA,OAExG,CAAC,eAEVT,OAAA,CAACJ,OAAO;MAACS,KAAK,EAAC,kHAA6B;MAAAD,QAAA,GAAC,u9BAGK,eAAAJ,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,+qBAGf,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,eAAAT,OAAA;QAAAM,QAAA,EAAAC,YAAA;QAAAC,UAAA;QAAAC,YAAA;MAAA,OAAI,CAAC,2kBAMnD,eAAAT,OAAA,CAACH,UAAU;QAACQ,KAAK,EAAC,mBAAmB;QAAAD,QAAA,GAAC,goBAEoB,eAAAJ,OAAA;UAAAM,QAAA,EAAAC,YAAA;UAAAC,UAAA;UAAAC,YAAA;QAAA,OAAI,CAAC,eAAAT,OAAA;U